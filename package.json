{
  "name": "silent-spring",
  "description": "silent-spring",
  "version": "0.2.0",
  "scripts": {
    "dev": "run-p dev:server dev:client",
    "dev:server": "nodemon --watch server server/index.js",
    "dev:client": "vite",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "vite:start": "vite",
    "vite:build": "vite build",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=development npm install && vite build",
    "start": "node server/index.js",
    "postinstall": "npm-run-all db:migrate db:seed",
    "test": "jest --verbose --watchAll",
    "lint": "eslint --ext .js,.jsx ."
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@react-three/drei": "^9.112.1",
    "@react-three/fiber": "^8.17.7",
    "@vscode/sqlite3": "^5.0.7",
    "auth0": "^2.44.0",
    "express": "^4.17.1",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.0.4",
    "knex": "^1.0.1",
    "react-redux": "^7.2.5",
    "tar": "^7.4.3",
    "three": "^0.168.0",
    "troika-3d": "^0.46.0",
    "troika-three-text": "^0.46.4",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.9.0",
    "eslint-config-eda": "^1.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.2.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^6.0.1",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "vite": "^5.4.7",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
